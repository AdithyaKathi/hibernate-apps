package com.mypack5;

public class Answer 
{
	private int empid;
	private String name;
	private Question ad;
	
	public int getEmpid() {
		return empid;
	}
	
	public void setEmpid(int empid) {
		this.empid = empid;
	}
	
	
	public String getName() {
		return name;
	}
	
	public void setName(String name) {
		this.name = name;
	}
	
	
	public Question getAd() {
		return ad;
	}
	
	public void setAd(Question ad) {
		this.ad = ad;
	}
	
	

}
